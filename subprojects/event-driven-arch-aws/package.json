{
  "name": "event-driven-arch-aws",
  "version": "1.0.0",
  "description": "Event-driven architecture example with AWS CDK, LocalStack, and comprehensive testing",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "cdk:deploy": "cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "localstack:start": "docker-compose up -d",
    "localstack:stop": "docker-compose down",
    "localstack:logs": "docker-compose logs -f",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:e2e": "jest --testPathPattern=e2e",
    "upload:test": "node scripts/upload-test-file.js",
    "trigger:event": "node scripts/trigger-event.js"
  },
  "keywords": [
    "aws",
    "cdk",
    "event-driven",
    "localstack",
    "sqs",
    "sns",
    "lambda",
    "dynamodb",
    "s3"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "aws-cdk": "^2.100.0",
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "aws-sdk": "^2.1500.0",
    "aws-sdk-mock": "^5.8.0",
    "localstack": "^2.3.0",
    "docker-compose": "^1.29.0"
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0"
  }
}
